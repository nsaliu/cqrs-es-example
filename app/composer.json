{
    "type": "project",
    "license": "proprietary",
    "name": "nsaliu/cqrs-es-example",
    "description": "CQRS and EventSouring example project",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "jphooiveld/eventsauce-bundle": "^1.3",
        "mark-gerarts/automapper-plus-bundle": "^1.3",
        "ramsey/uuid": "^4.0",
        "symfony/amqp-messenger": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/property-access": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "sensiolabs-de/deptrac-shim": "^0.8.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "fix": [
            "vendor/bin/php-cs-fixer fix src --allow-risky yes"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
