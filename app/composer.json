{
    "type": "project",
    "license": "proprietary",
    "name": "nsaliu/cqrs-es-example",
    "description": "CQRS and EventSouring example project",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "jphooiveld/eventsauce-bundle": "^1.3",
        "mark-gerarts/automapper-plus-bundle": "^1.3",
        "ramsey/uuid": "^4.0",
        "symfony/amqp-messenger": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.9.8",
        "symfony/framework-bundle": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan-symfony": "^0.12.7",
        "sensiolabs-de/deptrac-shim": "^0.8.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lint-yaml": [
            "bin/console lint:yaml src config --parse-tags"
        ],
        "deptrac": [
            "vendor/bin/deptrac"
        ],
        "csfixer": [
            "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --allow-risky yes"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyze src --level=8"
        ],
        "qa": [
            "@lint-yaml",
            "@csfixer",
            "@phpstan",
            "@deptrac"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
